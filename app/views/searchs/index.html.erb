<h1>search</h1>
<div class="container">
  <form action="searchs/index">
  <input type="text" name="search">
  <button type="submit">search</button>
  </form>

  <hr>
  <form action="store" method="get" id="form">
  <input type="hidden" name="dataId" value="" id="id">
  <div class="contains">
    <% @fj.each do |book| %>
    <div class="card">
      <div class="card-body">
        <%= image_tag(book['volumeInfo']['imageLinks']['smallThumbnail'], height: '60', width: '45', data: {"id" => book['id']}) %>
        <%= book['volumeInfo']['title'] %>
      </div>
    </div>

    <% end %>
  </div>
  </form>
</div>



